.header {
  padding: 0 30rpx;
  line-height: 1;
  background-color: #fff;
  width: 100%;
  text-align: center;
  padding:10px 0;
  margin-top: 10px;
}
.title {
  font-size: 52rpx;
}
.desc {
  margin-top: 10rpx;
  color: #888888;
  font-size: 28rpx;
}
.menu-list {
  width: 100%;
  display: flex;
  flex-direction: column;
  background-color: #fbf9fe;
  margin-top: 10rpx;
}
.menu-item {
  color: #000000;
  display: flex;
  background-color: #fff;
  margin: 10rpx 0rpx;
  flex-direction: column;
}
.menu-item-main {
  display: flex;
  padding: 40rpx;
  border-radius: 10rpx;
  align-items: center;
  font-size: 32rpx;
  justify-content: space-between;
}
.menu-item-main .menu-item-main-clock{
     width: 100%;
}
.menu-item-main .menu-item-main-clock text{
     background: #eee;
     display: inline-block;
     padding: 10rpx;
     margin-right: 10rpx;
     color: #666;
}
.menu-item-arrow {
  width: 96rpx;
  height: 96rpx;
  transition: 400ms;
}
.address{
     padding:0 10px;
     margin: 30rpx 0;
     font-size: 12px;
     color: #888888;
}
#map{
     display:flex;
	padding:0 10px;
     /* margin-top: 50rpx */
}
#map map{
     width: 100%;
     height: 200px;
}

/*输入框列表*/
#TT-ul{
     position: absolute;
     top: 124rpx;
     left: 0;
     width: 96%;
     margin: 0 2%;
     height: 500rpx;
     overflow-y: scroll;
     overflow-x: hidden;
     background-color: #fcfcfc;
}
#TT-ul .TT-items{
     width: 100%;
     min-height: 65rpx;
     line-height: 65rpx;
     color: #888888;
     font-size: 16px;
     padding: 10rpx;
}

/*指南针  */
.text {
  text-align: center;
  color: #000000;
  font-size: 40rpx;
  padding-top: 20rpx
}
.text text {
  display: block;
}
.pic image {
  width: 200rpx;
  height: 200rpx;
}